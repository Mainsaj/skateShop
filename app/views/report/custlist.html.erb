<h1>My Customers!</h1>
<table>  
<tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
</tr>

<% @customerlist .each do |s| %>

<tr>
    <td><%= s.first_name %></td>
    <td><%= s.last_name %></td>
    <td><%= s.phone %></td>
    <td><%= s.address %></td>
    <td><%= s.city %></td>
    <td><%= s.state %></td>
    <td><%= s.zip %></td>
</tr>
<% end %>

</table>