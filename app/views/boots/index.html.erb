<p id="notice"><%= notice %></p>

<h1>Boots</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model</th>
      <th>Cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @boots.each do |boot| %>
      <tr>
        <td><%= boot.brand %></td>
        <td><%= boot.model %></td>
        <td><%= boot.cost %></td>
        <td><%= link_to 'Show', boot %></td>
        <td><%= link_to 'Edit', edit_boot_path(boot) %></td>
        <td><%= link_to 'Destroy', boot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Boot', new_boot_path %>
